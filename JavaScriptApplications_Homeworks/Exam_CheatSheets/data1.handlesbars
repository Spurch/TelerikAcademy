<h2>DATA1</h2>
<a href="#/data/add">Add new</a>
{{#each this}}
  <h3><a href="#/todos?category={{category}}">{{category}}</a></h3>
  {{#each items}}
    <div class="data-box" data-id="{{id}}">
      <label class="checkbox">
        {{#if state}}
          <input type="checkbox" checked />
        {{else}}
          <input type="checkbox" />
        {{/if}}
        <strong>{{text}}</strong>
      </label>
    </div>
  {{/each}}
{{/each}}